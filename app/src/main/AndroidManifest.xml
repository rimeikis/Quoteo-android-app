<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.weightlossdiary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/quoteo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/quoteo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".CreateAccActivity"></activity>
        <activity
            android:name=".QuoteListActivity"
            android:label="Quoteo" />
        <activity
            android:name=".SubmitQuoteActivity"
            android:label="Quoteo" />
        <activity
            android:name=".MainActivity"
            android:label="Quoteo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>